{
  "version": 2,
  "name": "vserver",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/index.js"
    }
  ],
  "alias": [
    "vserver"
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "1337",
    "DATABASE_URL_USER": "@visiona-api-db",
    "CORS_ORIGIN": "https://visiona.cat",
    "API_KEY": "@visiona-api-key"
  }
}